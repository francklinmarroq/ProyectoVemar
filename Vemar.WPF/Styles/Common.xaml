<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Colors-->
    <Color x:Key="Primary">#18d8f5</Color>
    <Color x:Key="Secondary">#09DC7D</Color>
    <Color x:Key="Accent">#F4FAFB</Color>
    <Color x:Key="Text">#23262B</Color>

    <!--Brushes-->
    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource Primary}"/>
    <SolidColorBrush x:Key="SecondaryBrush" Color="{StaticResource Secondary}"/>
    <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource Accent}"/>
    <SolidColorBrush x:Key="BackgroundBrush" Color="#23262B"/>
</ResourceDictionary>
